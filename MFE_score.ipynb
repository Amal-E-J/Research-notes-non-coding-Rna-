   "source": [
    "from Bio.Seq import Seq\n",
    "from Bio.SeqUtils import MeltingTemp\n",
    "\n",
    "rna_seq = Seq(\"GTTAACTTGCCTTTGACCTCTTCTTTCTGTTCATGTGTGTTAGTAATTTGCTGTC\")\n",
    "\n",
    "mfe = MeltingTemp.Tm_Wallace(rna_seq.transcribe().reverse_complement())\n",
    "print(f\"MFE: {mfe}\")\n"
   ]
